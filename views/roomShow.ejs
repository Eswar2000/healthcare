<div id="main-content" class="container">
    <h2>Welcome Employee</h2>
    <div class="row">
        <h2>All Rooms</h2>
        <br>
        <table>
            <thead>
                <td class="cell">Room No</td>
                <td class="cell">Type</td>
                <td class="cell">Patient Name</td>
                <td class="cell">In Date</td>
                <td class="cell">Expected Out Date</td>
            </thead>
            <tbody>
                <%roomhist.forEach(function(entry) {%>
                <tr>
                    <td class="cell"><%=entry.rNo%></td>
                    <td class="cell"><%=entry.rType%></td>
                    <td class="cell"><%=entry.PName%></td>
                    <td class="cell"><%=String(entry.rInDate).substring(0,10)%></td>
                    <td class="cell"><%=String(entry.rOutDate).substring(0,10)%></td>
                </tr>
                <%});%>
            </tbody>
        </table>
        <br>
        <button type="submit" class="btn btn-primary" onclick="roomSearch()">Search</button>
    </div>	
</div><!--End of #main-content -->